{
  "name": "notion-homeassistant",
  "version": "1.0.0",
  "description": "A Home Assistant integration for Notion",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "test": "jest",
    "start": "ts-node src/index.ts",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc",
    "lint": "eslint . --ext .ts",
    "build": "tsc",
    "format": "prettier --write src/**/*.ts",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@brittonhayes/homeassistant-ts": "^0.1.16",
    "@notionhq/client": "^2.2.2",
    "@types/node-fetch": "2",
    "dotenv": "^16.0.3",
    "node-fetch": "2"
  },
  "devDependencies": {
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "ts-node": "^10.8.2",
    "typedoc": "^0.23.23",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.9.4"
  }
